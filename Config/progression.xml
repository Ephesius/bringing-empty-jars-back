<!-- Replaces unlocking of food items with their respective "food item/empty jar" bundles -->

<BringingEmptyJarsBack>
    <!-- Corn Bread -->
    <set xpath="/progression/crafting_skills/crafting_skill[@name='craftingFood']/display_entry[contains(@icon, 'foodCornBread')]/unlock_entry[contains(@item, 'foodCornBread')]/@item">foodCornBreadEmptyJar,drinkJarGoldenRodTea</set>
    <set xpath="/progression/crafting_skills/crafting_skill[@name='craftingFood']/effect_group/passive_effect[contains(@tags, 'foodCornBread')]/@tags">foodCornBreadEmptyJar,drinkJarGoldenRodTea</set>

